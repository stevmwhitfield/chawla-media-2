    "gatsby-transformer-sharp": "^3.14.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.41.1"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "prettier": "2.3.2"
